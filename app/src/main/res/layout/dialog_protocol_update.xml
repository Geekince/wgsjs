<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent"
    tools:background="@color/black1"
    >

  <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="280dp"
      android:layout_height="wrap_content"
      android:background="@drawable/bg_white_dialog"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.5"
      >

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/protocol_title"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/tv_content_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:text="@string/protocol_content_1"
        android:textColor="@color/black1"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_title"
        />

    <TextView
        android:id="@+id/tv_content_temp_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:text="@string/protocol_content_temp1"
        android:textColor="@color/black1"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_content_1"
        />


    <TextView
        android:id="@+id/tv_content_temp_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/protocol_content_temp2"
        android:textColor="@color/yb_color"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@id/tv_content_temp_1"
        app:layout_constraintTop_toBottomOf="@id/tv_content_1"
        />


    <TextView
        android:id="@+id/tv_content_temp_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/protocol_content_and"
        android:textColor="@color/black1"
        android:textSize="11sp"
        app:layout_constraintStart_toEndOf="@id/tv_content_temp_2"
        app:layout_constraintTop_toBottomOf="@id/tv_content_1"
        />
    <TextView
        android:id="@+id/tv_content_temp_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/protocol_content_temp4"
        android:textColor="@color/yb_color"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@id/tv_content_temp_3"
        app:layout_constraintTop_toBottomOf="@id/tv_content_1"
        />


    <TextView
        android:id="@+id/tv_content_temp_5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:text="@string/protocol_content_temp5"
        android:textColor="@color/black1"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/tv_content_temp_1"
        app:layout_constraintTop_toBottomOf="@id/tv_content_temp_2"
        />

    <View
        android:id="@+id/temp_view_1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="40dp"
        android:background="@color/grey1"
        app:layout_constraintTop_toBottomOf="@id/tv_content_temp_5"
        />

    <View
        android:id="@+id/view3"
        android:layout_width="1dp"
        android:layout_height="30dp"
        android:background="@color/grey1"
        app:layout_constraintBottom_toBottomOf="@id/tvDone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/tvDone"
        />
    <TextView
        android:id="@+id/tvCancel"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/can_not_done"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/temp_view_1"
        />

    <TextView
        android:id="@+id/tvDone"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/agree"
        android:gravity="center"
        android:textColor="@color/yb_color"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@id/temp_view_1"
        />


  </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>